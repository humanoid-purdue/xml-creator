                    <body name="r_knee" pos="0 0 -0.2286" quat="1 0 0 0">
                        <joint name="r_knee" axis="0 1 0" range="-3 3"/>
                        <inertial pos ="0.10360317718212292 -0.004941471406058302 -0.026917890990596487" quat="1 0 0 0" mass="3.4080436" diaginertia="0.0025045269244811904 0.0025045269244811904 0.0025045269244811904"/>
                        <geom type="mesh" class="visual" mesh="r_knee"/>

                        <body name="r_foot_pitch" pos="0 -0.00635 -0.3366" quat="1 0 0 0">
                            <joint name="r_foot_pitch" axis="0 1 0" range="-0.34 0.34"/>
                            <inertial pos ="0.0035560000000000036 0.0 0.0" quat="1 0 0 0" mass="0.8572895" diaginertia="6.937947084078097e-07 6.937947084078097e-07 6.937947084078097e-07"/>
                            <geom type="mesh" class="visual" mesh="r_foot_pitch"/>

                            <body name="r_foot_roll" pos="0.0889 0 0" quat="1 0 0 0">
                                <joint name="r_foot_roll" axis="1 0 0" range="-1 1"/>
                                <inertial pos ="-0.005602856670571986 0.0 0.028940310004584514" quat="1 0 0 0" mass="1.786451" diaginertia="9.93476609435633e-05 9.93476609435633e-05 9.93476609435633e-05"/>
                                <geom type="mesh" class="visual" mesh="r_foot_roll"/>

                                <geom type="mesh" class="collision" mesh="foot_col"/>
                            </body>

                        </body>
                    </body>
                </body>
            </body>
        </body>


    </body>


</worldbody>

<actuator>
  <position kp="400.0" class="nemo" name="l_hip_pitch" joint="l_hip_pitch" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="l_hip_roll" joint="l_hip_roll" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="l_hip_yaw" joint="l_hip_yaw" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="l_knee" joint="l_knee" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="l_foot_pitch" joint="l_foot_pitch" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="l_foot_roll" joint="l_foot_roll" forcerange="-30 30" inheritrange="1"/>

  <position kp="400.0" class="nemo" name="r_hip_pitch" joint="r_hip_pitch" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="r_hip_roll" joint="r_hip_roll" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="r_hip_yaw" joint="r_hip_yaw" forcerange="-30 30" inheritrange="1"/>
