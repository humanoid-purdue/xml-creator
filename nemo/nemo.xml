  <velocity kv="40.0" class="nemo" name="r_foot_pitch_v" joint="r_foot_pitch" forcerange="-30 30" ctrlrange="-10 10"/>
  <velocity kv="40.0" class="nemo" name="r_foot_roll_v" joint="r_foot_roll" forcerange="-30 30" ctrlrange="-10 10"/>
