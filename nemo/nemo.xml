  <position kp="400.0" class="nemo" name="r_foot_pitch" joint="r_foot_pitch" forcerange="-30 30" inheritrange="1"/>
  <position kp="400.0" class="nemo" name="r_foot_roll" joint="r_foot_roll" forcerange="-30 30" inheritrange="1"/>

  <velocity kv="40.0" class="nemo" name="l_hip_pitch_v" joint="l_hip_pitch" forcerange="-30 30" ctrlrange="-10 10"/>
  <velocity kv="40.0" class="nemo" name="l_hip_roll_v" joint="l_hip_roll" forcerange="-30 30" ctrlrange="-10 10"/>
  <velocity kv="40.0" class="nemo" name="l_hip_yaw_v" joint="l_hip_yaw" forcerange="-30 30" ctrlrange="-10 10"/>
  <velocity kv="40.0" class="nemo" name="l_knee_v" joint="l_knee" forcerange="-30 30" ctrlrange="-10 10"/>
