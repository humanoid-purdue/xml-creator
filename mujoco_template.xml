        <joint name="r_hip_roll" axis="1 0 0" range="-1.0472 0.279253"/>
        <geom type="mesh" class="visual" mesh="r_hip_roll"/>
        <body name="r_hip_yaw" pos="0.0508 0 -0.09525" quat="1 0 0 0">
