              <geom type="mesh" class="visual" mesh="l_foot_pitch"/>
              <body name="r_foot_roll" pos="0.05176 0.0381 0" quat="1 0 0 0">
                <inertial pos="-0.0056 0 0.02894" quat="1 0 0 0" mass="2" fullinertia="0.4 0.4 0.4"/>
