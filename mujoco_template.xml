                <site name="left_foot_p1" pos="0.05 0.0 -0.0762"/>
                <site name="left_foot_p2" pos="-0.05 0.0 -0.0762"/>
                <site name="left_foot_p3" pos="0.0 0.05 -0.0762"/>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>


    <body name="r_hip_pitch" pos="0 -0.07985 -0.0762" quat="1 0 0 0">
